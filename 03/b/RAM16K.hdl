CHIP RAM16K {
    IN in[16], load, address[14];
    OUT out[16];

    PARTS:
    DMux4Way(in=load, sel=address[0..1], a=l0, b=l1, c=l2, d=l3);
    RAM4K(in=in, load=l0, address=address[2..13], out=o0);
    RAM4K(in=in, load=l1, address=address[2..13], out=o1);
    RAM4K(in=in, load=l2, address=address[2..13], out=o2);
    RAM4K(in=in, load=l3, address=address[2..13], out=o3);
    Mux4Way16(a=o0, b=o1, c=o2, d=o3, sel=address[0..1], out=out);
}
